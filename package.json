{
  "name": "@proton/cli",
  "description": "Proton CLI",
  "version": "0.1.7",
  "author": "Syed Jafri @jafri",
  "bin": {
    "proton": "./bin/run"
  },
  "bugs": "https://github.com/ProtonProtocol/proton-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.8.16",
    "@oclif/config": "^1.18.3",
    "@oclif/core": "^1.6.4",
    "@oclif/plugin-help": "^3",
    "@proton/js": "^26.1.2",
    "@proton/mnemonic": "^0.1.3",
    "@types/rimraf": "^3.0.0",
    "cli-ux": "^5.5.1",
    "colors": "^1.4.0",
    "conf": "^10.1.2",
    "elliptic": "^6.5.3",
    "eosio-mnemonic": "^0.4.0",
    "eth-sig-util": "^3.0.1",
    "hasbin": "^1.2.3",
    "ini": "^2.0.0",
    "isomorphic-git": "^1.8.0",
    "keytar": "^7.9.0",
    "node-fetch": "^2.6.1",
    "rimraf": "^3.0.2",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4.2.14",
    "@types/debug": "^4.1.5",
    "@types/elliptic": "^6.4.12",
    "@types/ini": "^1.3.30",
    "@types/mocha": "^8.2.0",
    "@types/node": "^10",
    "@types/node-fetch": "^2.5.7",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ProtonProtocol/proton-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "proton",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "ProtonProtocol/proton-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "testbase": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "test": "npm run testbase -- \"test/**/*.test.ts\"",
    "testsome": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\" --exclude \"**/boilerplate.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
